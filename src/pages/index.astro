---
import Whale from '../components/Whale.astro';
import "../styles/global.css";
import BaseHead from '../components/BaseHead.astro';
import { SITE_DESCRIPTION, SITE_TITLE } from '../config';

// Welcome to Astro! Everything between these triple-dash code fences
// is your "component front matter". It never runs in the browser.
console.log('This runs in your terminal, not the browser!');
---
<!-- Below is your "component template." It's just HTML, but with
     some magic sprinkled in to help you build great templates. -->
<html lang="es">
  <head>
    <BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION}/>
  </head>
  <body>
    <h1>Tania Espitia - UI/UX Designer</h1>
    <Whale />
    <blockquote>"Se tu propia inspiración"</blockquote>
    <small>¡Muy pronto!</small>
  </body>
</html>
<style>
  h1 {
    color: #002C44;
  }
</style>